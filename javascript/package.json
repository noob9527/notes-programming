{
  "name": "trap",
  "version": "1.0.0",
  "scripts": {
    "test": "node --harmony node_modules/.bin/ava ecmascript6 --verbose",
    "start": "node --harmony node_modules/.bin/ava ecmascript6 --verbose --watch",
    "es5test": "node node_modules/.bin/mocha --watch --ui bdd --recursive ecmascript5"
  },
  "author": "xy",
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.0.0",
    "@ava/babel-preset-transform-test-files": "^2.0.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "ava": "^0.17.0",
    "mocha": "^2.5.3",
    "chai": "^3.5.0",
    "should": "^9.0.2"
  },
  "ava": {
    "babel": {
      "presets": [
        "@ava/stage-4",
        "@ava/transform-test-files"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties"
      ]
    }
  }
}
